{
  "name": "vite-vue3-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "yarn typecheck && vite build",
    "debug": "vite --debug",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write \"./**/*.{html,vue,ts,js,json,md}\"",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "cy:test": "npx cypress open",
    "cy:run": "npx cypress run"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.32.0",
    "@soybeanjs/vue-admin-tab": "^1.0.5",
    "@vueuse/core": "^9.6.0",
    "@vueuse/math": "^9.9.0",
    "@xstate/vue": "^2.0.0",
    "axios": "^1.1.3",
    "chroma-js": "^2.4.2",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "pinia": "^2.0.22",
    "ramda": "^0.28.0",
    "sweetalert2": "^11.5.2",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.5",
    "xstate": "^4.35.1"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.120",
    "@iconify/vue": "^4.0.0",
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@types/chroma-js": "^2.1.4",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^18.7.23",
    "@types/ramda": "^0.28.20",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vitest/coverage-c8": "^0.23.4",
    "autoprefixer": "^10.4.12",
    "cypress": "^10.9.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.5.1",
    "happy-dom": "^6.0.4",
    "naive-ui": "^2.33.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4",
    "unplugin-icons": "^0.14.11",
    "unplugin-vue-components": "^0.22.7",
    "unplugin-vue-macros": "^0.12.0",
    "vfonts": "^0.0.3",
    "vite": "^3.2.4",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-purge-icons": "^0.9.2",
    "vitest": "^0.23.4",
    "vue-tsc": "^0.40.4"
  }
}
